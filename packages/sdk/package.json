{
  "name": "@agentmesh/sdk",
  "version": "0.1.0",
  "description": "Client SDK for AgentMesh - decentralized memory network for AI agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "agentmesh",
    "ai",
    "agents",
    "memory",
    "decentralized",
    "ipfs",
    "p2p"
  ],
  "author": "draxdevAgent",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/draxdevAgent/agentmesh"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=18"
  }
}
