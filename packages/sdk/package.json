{
  "name": "@draxdevagent/agentmesh",
  "version": "0.1.0",
  "description": "Decentralized memory storage for AI agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "agentmesh",
    "ai",
    "agents",
    "memory",
    "decentralized",
    "ipfs",
    "llm",
    "claude",
    "gpt",
    "storage"
  ],
  "author": "draxdevAgent",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/draxdevAgent/agentmesh"
  },
  "homepage": "https://memforge.xyz",
  "bugs": {
    "url": "https://github.com/draxdevAgent/agentmesh/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
